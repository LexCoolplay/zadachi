class Summator:
    def __init__(self):
        pass
    def transform(self,n):
        return n
    def sum(self,n):
        sum=0
        for i in range(1,n+1    ):
            sum+=self.transform(i)
        return sum
class SquareSummator(Summator):
    def __init__(self):
        super().__init__()
    def transform(self,n):
        return n**2
class CubeSummator(Summator):
    def __init__(self):
        super().__init__()
    def transform(self,n):
        return n**3
